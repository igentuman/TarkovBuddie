<Application x:Class="TARKIT.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TARKIT"
             xmlns:views="clr-namespace:TARKIT.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <views:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <views:BoolToColorConverter x:Key="BoolToColorConverter"/>
        <views:StringToImageConverter x:Key="StringToImageConverter"/>
    </Application.Resources>
</Application>
